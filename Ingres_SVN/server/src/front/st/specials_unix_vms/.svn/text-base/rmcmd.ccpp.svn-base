PART   rmcmd

/*
** ingres!bin
*/

   DIRECTORY ingres!bin
   BUILD_DIR (ING_BUILD)!bin
# ifdef VMS
      SCRIPT rmcmd
      EXECUTABLE rmcmdv
# endif /* VMS */
      EXECUTABLE rmcmd
      EXECUTABLE rmcmdin
      EXECUTABLE rmcmdout
      EXECUTABLE rmcmdstp

/*
** ingres!utility
*/
 
   DIRECTORY ingres!utility
   BUILD_DIR (ING_BUILD)!utility
# ifdef VMS
      SCRIPT iirunrmcmd
# endif /* VMS */
      EXECUTABLE rmcmdrmv
      EXECUTABLE rmcmdgen

/*
** ingres!vdba
*/

   DIRECTORY ingres!vdba
   BUILD_DIR (ING_BUILD)!vdba
      FILE makimau.sql
# ifdef FULL_RELEASE
      FILE makiman.sql
      FILE makimag.sql
      FILE makimav.sql
# endif /* FULL_RELEASE */
