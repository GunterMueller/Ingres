NETSCAPE_API_FLAG 	= "" ;
APACHE_API_FLAG 	= "-DOSX" ;
VERS 	= "int_osx" ;
#OUTARCH = "-arch i386 -arch ppc" ;
OUTARCH = "" ;
CC 	= "gcc" ;
#AR	= "ld $(OUTARCH) -r -o" ;
#RANLIB	= true ;
CCDEBUG = "-g" ;
CPP 	= "/usr/bin/cpp" ;
CCLDMACH 	= "$(OUTARCH) -framework System" ;
#CCLDMACH 	= "-framework System" ;
SHLIBLD		= libtool ;
CCSHLDMACH	= "-dynamic -framework System -undefined dynamic_lookup -dead_strip" ;
CCLDSERVER 	= "" ;
CCPICFLAG 	= "-fPIC" ;
CCVERSFLAG	= "-D$(VERS)" ;
CCMACH 	= "$(CCVERSFLAG) -DDARWIN $(OUTARCH) -dynamic -fno-common -fsigned-char -fasm-blocks " ;
#CCMACH 	= "$(CCVERSFLAG) -DDARWIN -dynamic -fno-common -fsigned-char -fasm-blocks " ;
SLSFX 	= "dylib" ;
OBJSFX		= "o" ;
LIBSFX		= "a" ;
MWSLSFX 	= "dylib" ;
CGISFX 	= "cgi" ;
LDLIBPATH 	= "/usr/lib /usr/local/lib" ;
LDLIBPATH64 	= "" ;
LIBMACH 	= "" ;
LDLIBMACH 	= "-bind_at_load -framework CoreServices -framework DirectoryService -framework Security -lgcc_s.1" ;
#LDLIBMACH 	= "-v -lm -lc -ldl -lgcc -bind_at_load -undefined dynamic_lookup -framework CoreServices -framework DirectoryService -framework Security" ;
LD_ICE_FLAGS 	= "" ;
ICE_EXT		= so ;
CGI_EXT		= cgi ;
LONGLONG	= long long ;
OPTIM		= ;
IIOPTIM		?= "-O" ;
IINOOPTIM 	?= "-O0" ;
IIL1OPTIM 	?= "-O1" ;
SHELLHEADER 	= ""#!/bin/sh"" ;
BSD42 default = -DBSD42 ;
LEX = flex ;
DIRENT default = -DDIRENT ;
DIRFUNC default = -DDIRFUNC ;
NO_DIRFUNC default = -DNO_DIRFUNC ;
SYS5 default = -DSYS5 ;
CXX	= "g++" ;
CXX_DEFS	= "" ;
UNDEFFLAG  = "-u _" ;
XERC_DEFS	= "-O -DXML_USE_NATIVE_TRANSCODER -DXML_USE_INMEM_MESSAGELOADER -DXML_USE_NETACCESSOR_SOCKET" ;
XERC_LIBS	= "" ;
DBMS_STACKSIZE = 131072 ;
platform_string = "Intel Mac OS X (Darwin)" ;
extension_string = $(config_string) ;
