if $(SVR_SHLIBS)
{
    IISVRSHLIBRARY $(SHADFLIB) ;
    IINEEDLIBS $(SHADFLIB) : ADFLIB ;

    IISVRSHLIBRARY $(SHBECOMPATLIB) ;
    IINEEDLIBS $(SHBECOMPATLIB) : COMPATLIB ;

    IISVRSHLIBRARY $(SHDMFLIB) ;
    IINEEDLIBS $(SHDMFLIB) : DMFLIB ;

    IISVRSHLIBRARY $(SHSCFLIB) ;
    IINEEDLIBS $(SHSCFLIB) : SCFLIB ;

    IISVRSHLIBRARY $(SHQEFLIB) ;
    IINEEDLIBS $(SHQEFLIB) : QEFLIB ;

    IISVRSHLIBRARY $(SHPSFLIB) ;
    IINEEDLIBS $(SHPSFLIB) : PSFLIB ;

    IISVRSHLIBRARY $(SHOPFLIB) ;
    IINEEDLIBS $(SHOPFLIB) : OPFLIB ;

    IISVRSHLIBRARY $(SHRDFLIB) ;
    IINEEDLIBS $(SHRDFLIB) : RDFLIB ;

    IISVRSHLIBRARY $(SHQSFLIB) ;
    IINEEDLIBS $(SHQSFLIB) : QSFLIB ;

    IISVRSHLIBRARY $(SHTPFLIB) ;
    IINEEDLIBS $(SHTPFLIB) : TPFLIB ;

    IISVRSHLIBRARY $(SHRQFLIB) ;
    IINEEDLIBS $(SHRQFLIB) : RQFLIB ;

    IISVRSHLIBRARY $(SHGCFLIB) ;
    IINEEDLIBS $(SHGCFLIB) : GCFLIB ;

    IISVRSHLIBRARY $(SHGWFLIB) ;
    IINEEDLIBS $(SHGWFLIB) : GWFLIB ;

    IISVRSHLIBRARY $(SHSXFLIB) ;
    IINEEDLIBS $(SHSXFLIB) : SXFLIB ;

    IISVRSHLIBRARY $(SHCUFLIB) ;
